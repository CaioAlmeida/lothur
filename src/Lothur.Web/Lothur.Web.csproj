<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp1.1</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Cristiano Campos(cristiano.campos@outlook.com)</Authors>
    <Company>Cristiano Campos</Company>
    <PackageProjectUrl>https://github.com/camposdelima/lothur</PackageProjectUrl>
    <RepositoryUrl>https://github.com/camposdelima/lothur</RepositoryUrl>
    <Description>Web GUI Automation Framework</Description>
    <AssemblyVersion>1.0.1.0</AssemblyVersion>
    <FileVersion>1.0.1.0</FileVersion>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Lothur.Web.Tests\**" />
    <EmbeddedResource Remove="Lothur.Web.Tests\**" />
    <None Remove="Lothur.Web.Tests\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CoreCompat.Selenium.WebDriver" Version="2.54.0-beta002" />
  </ItemGroup>

</Project>